{
	"ConnectionStrings": {
		"SqlServer": "Server=localhost;Initial Catalog=sql_server_db;Integrated Security=True;MultipleActiveResultSets=true",
		"PostgreSql": "Host=localhost;Username=postgres;Password=postgres;Database=postgres_db"
	},
	"SqlServer": {
		"SuperUser": "sa",
		"Database": "sql_server_db",
		"Scheme": "dbo"
	},
	"PostgreSql": {
		"SuperUser": "postgres",
		"Database": "postgres_db",
		"Scheme": "public",
		"IncludeTables": [],
		"ExcludeTables": []
	},
	"TasksPlan": {
		"CreateScheme": true,
		"CreateTables": true,
		"CopyRows": true,
		"CreateTableConstraints": true,
		"CreateTableIndexes": true
	},
	"Logging": {
		"LogLevel": {
			"Default": "Trace",
			"System": "Warning",
			"Microsoft": "Warning"
		},
		"Console": {
			"FormatterName": "simple",
			"FormatterOptions": {
				"IncludeScopes": false,
				"TimestampFormat": "yyyy-MM-dd HH\\:mm\\:ss.fff ",
				"UseUtcTimestamp": true,
				"SingleLine": true
			}
		},
		"File": {
			"IsEnabled": true,
			"FormatterName": "log",
			"FileName": "logs-",
			"RetainedFileCountLimit": "5",
			"LogDirectory": "./logs"
		}
	}
}
